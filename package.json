{
  "name": "nuxt-material-admin",
  "version": "1.0.0",
  "description": "vue-material-admin integration with Nuxt.js",
  "author": "Mohamed Eddami",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "debug": "cross-env NODE_ENV=development DEBUG=koa* nodemon --harmony server/index.js --watch server",
    "build": "nuxt build",
    "knex": "knex",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "seed:devmake": "cross-env NODE_ENV=development knex seed:make",
    "seed:testmake": "cross-env NODE_ENV=test knex seed:make",
    "seed": "knex seed:run",
    "migrate:make": "knex migrate:make",
    "migrate": "knex migrate:latest",
    "makedb": "if test -f dev.sqlite3; then rm dev.sqlite3; fi; npm run migrate && npm run seed",
    "makedb:test": "if test -f test.sqlite3; then rm test.sqlite3; fi; npm run migrate && npm run seed",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest --maxWorkers=4 --force-exit"
  },
  "dependencies": {
    "bookshelf": "^0.15.1",
    "cross-env": "^5.2.0",
    "dotenv": "^8.1.0",
    "font-awesome": "^4.7.0",
    "knex": "^0.19.3",
    "koa": "^2.8.1",
    "lodash.truncate": "^4.4.2",
    "nuxt": "^2.4.2",
    "quill": "^1.3.6",
    "roboto-fontface": "*",
    "sqlite3": "^4.1.0",
    "tarn": "^2.0.0",
    "vee-validate": "^2.1.7",
    "vue-fullcalendar": "^1.0.9",
    "vue-perfect-scrollbar": "^0.1.0",
    "vue-quill-editor": "^3.0.6",
    "vuetify": "^1.4.4",
    "vuetify-loader": "^1.2.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
